@model Department
@{
    ViewData["Title"] = "Delete";
}

    <h1>Delete Department</h1>

    <form method="post" asp-action="Delete">
    @Html.AntiForgeryToken()
    <input asp-for="Uid" hidden name="Uid" class="form-control" />
    <div>
        <label asp-for="Name" class="form-label"></label>
        <input asp-for="Name"disabled name="Name" class="form-control" />
    </div>

    <div>
        <label asp-for="Description" class="form-label"></label>
        <input asp-for="Description"disabled name="Description" class="form-control" />

    </div>

    <button type="submit" class="btn btn-outline-warning">Delete</button>
    <a asp-action="Index" class="btn btn-primary">Back</a>
</form>

